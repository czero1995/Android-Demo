<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 主色：宝蓝色 -->
    <color name="mainblue">#0757c1</color>
    <!-- 辅助色：浅蓝色 -->
    <color name="lightblue">#5da0ef</color>
    <!-- 玫红色 -->
    <color name="rosered">#FE394B</color>
    <!-- 橙黄色 -->
    <color name="orangeyellow">#ffcc00</color>
    <!-- 背景灰色 -->
    <color name="lightgray">#f4f4f4</color>
    <!-- 灰色 -->
    <color name="normalgray">#AAAAAA</color>
    <!-- 深灰 -->
    <color name="darkgray">#585858</color>
    <!-- 文字搭配 -->
    <color name="charactersmix">#999999</color>
    <!-- 文字内容 -->
    <color name="characters">#666666</color>
    <!-- 白色 -->
    <color name="white">#FFFFFF</color>
    <!-- 浅绿色 -->
    <color name="green">#DFFCAC</color>


    <!-- 透明 -->
    <color name="transparent">#00000000</color>
    <!-- 半透明浅灰色背景 -->
    <color name="lucentgray">#3f000000</color>


    <integer-array name="progress_colors">
        <item>@color/mainblue</item>
        <item>@color/orangeyellow</item>
        <item>@color/rosered</item>
        <item>@color/white</item>
    </integer-array>





</resources>
